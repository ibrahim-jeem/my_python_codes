#This math code is a simple form of calculating the total sum of a divirsor number.

def sum_divirsor(n):
  sum = 0
  for i in range (n):
    if (n % i) == 0:
      sum = i + sum
  return sum

print("What is the number: ")
print("The total sum is = ", sum_divirsor(int(input())))



#This lines of code will generate the sums of square of a number
def square(n):
    return n*n

def sum_squares(x):
    sum = 0
    for n in range(1, x):
        sum += n*n
    return sum

print("Give the number: ")
print("The result is = ", sum_squares(int(input())))
